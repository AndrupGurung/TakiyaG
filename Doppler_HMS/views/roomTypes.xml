<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_hms_room_types_form" model="ir.ui.view">
        <field name="name">hms.room_types.form</field>
        <field name="model">hms.room_types</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="room_types"/>
                        <field name="date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_hms_room_types_tree" model="ir.ui.view">
        <field name="name">hms.room_types.tree</field>
        <field name="model">hms.room_types</field>
        <field name="arch" type="xml">
            <tree>
                <field name="room_types"/>
                <field name="date"/>
            </tree>
        </field>
    </record>

</odoo>