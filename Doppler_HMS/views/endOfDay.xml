<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_hms_end_of_day_form" model="ir.ui.view">
        <field name="name">hms.end_of_day.form</field>
        <field name="model">hms.end_of_day</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_hms_end_of_day_tree" model="ir.ui.view">
        <field name="name">hms.end_of_day.tree</field>
        <field name="model">hms.end_of_day</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>


</odoo>