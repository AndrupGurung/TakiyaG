<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_hms_room_features_form" model="ir.ui.view">
        <field name="name">hms.room_features.form</field>
        <field name="model">hms.room_features</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="features"/>
                        <field name="date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_hms_room_features_tree" model="ir.ui.view">
        <field name="name">hms.room_features.tree</field>
        <field name="model">hms.room_features</field>
        <field name="arch" type="xml">
            <tree>
                <field name="features"/>
                <field name="date"/>
            </tree>
        </field>
    </record>

</odoo>